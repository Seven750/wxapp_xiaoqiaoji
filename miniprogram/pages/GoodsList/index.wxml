<wux-index height="100%" bind:change="onChange">
    <wux-index-item wx:for="{{ alphabet }}" wx:key="" name="{{ item.initial }}">
        <view class="demo-item" wx:for="{{ item.cells }}" wx:for-item="cell" wx:key="" id="{{index}}" bindtap='onselect'>{{ cell }}</view>
    </wux-index-item>
</wux-index>
